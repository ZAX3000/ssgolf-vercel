{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",

  "buildCommand": "vite build",
  "outputDirectory": "dist",

  "rewrites": [
    {
      "source": "/api/datagolf/:path*",
      "destination": "/api/datagolf"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],

  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-node@2.3.0"
    }
  }
}
