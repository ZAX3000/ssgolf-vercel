{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",

  "devCommand": null,

  "buildCommand": "npm run build",
  "outputDirectory": "dist",

  "rewrites": [
    { 
      "source": "/api/datagolf/:path*",
      "destination": "/api/datagolf"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
